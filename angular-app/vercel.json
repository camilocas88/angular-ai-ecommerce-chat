{
  "version": 2,
  "buildCommand": "npm install --legacy-peer-deps && npm run build:spa",
  "outputDirectory": "dist/ecommerce",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/.*",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}









