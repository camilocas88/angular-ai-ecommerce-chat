{
  "version": 2,
  "buildCommand": "npm install --legacy-peer-deps && npm run build:spa",
  "outputDirectory": "dist/ecommerce",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}









